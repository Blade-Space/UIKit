{
  "name": "server-os-uikit",
  "description": "UIKit for server os",
  "author": "egorkaBurkenya <sleepyegorka@gmail.com>, nik19ta <nik19ta.me@gmail.com>",
  "keywords": [
    "react",
    "components",
    "ui",
    "uikit",
    "server-os"
  ],
  "version": "0.1.7",
  "private": false,
  "main": "dist/uikit.es.js",
  "module": "dist/uikit.es.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Blade-Space/UIKit.git"
  },
  "dependencies": {
    "@use-gesture/react": "^10.2.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "react-spring": "^9.5.5",
    "react-three-fiber": "^6.0.13",
    "react-use-gesture": "^9.1.3",
    "simplebar-react": "^2.4.3",
    "vite-plugin-css-modules": "^0.0.1",
    "vite-plugin-dts": "^1.7.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "vite:start": "vite",
    "build": "react-scripts build",
    "vite:build": "vite build && tsc  && cp package.json README.md ./dist",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "sb": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -o docs-build",
    "npm:build": "rm -rf dist && NODE_ENV=production babel src/lib --out-dir dist --copy-files"
  },
  "storybook-deployer": {
    "gitUsername": "GH Pages Bot",
    "gitEmail": "burkenya.egor@gmail.com",
    "commitMessage": "Deploy Storybook to GitHub Pages"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.5.13",
    "@storybook/addon-essentials": "^6.5.13",
    "@storybook/addon-interactions": "^6.5.13",
    "@storybook/addon-links": "^6.5.13",
    "@storybook/builder-webpack5": "^6.5.13",
    "@storybook/manager-webpack5": "^6.5.13",
    "@storybook/node-logger": "^6.5.13",
    "@storybook/preset-create-react-app": "^4.1.2",
    "@storybook/react": "^6.5.13",
    "@storybook/testing-library": "^0.0.13",
    "@testing-library/dom": "^8.19.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.0",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@vitejs/plugin-react": "^3.0.0",
    "babel-plugin-named-exports-order": "^0.0.2",
    "c8": "^7.12.0",
    "eslint": "^8.30.0",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-css-module-hints": "^1.0.1",
    "jsdom": "^20.0.3",
    "prop-types": "^15.8.1",
    "react-test-renderer": "^18.2.0",
    "typescript": "^4.8.4",
    "typings-for-css-modules-loader": "^1.7.0",
    "vite": "^4.0.3",
    "vite-plugin-svgr": "^2.4.0",
    "vite-tsconfig-paths": "^4.0.3",
    "vitest": "^0.26.2",
    "webpack": "^5.74.0"
  }
}
